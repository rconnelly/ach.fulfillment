@model Ach.Fulfillment.Web.Areas.Manage.Models.UserModel

@{
    ViewBag.Title = "Create User";
}

@using (Html.BeginForm("Create", "User", new { area = "Manage" }))
{
    <fieldset>
        <legend>Create User</legend>

        @Html.Partial("_userDetails", this.Model)
        <p>
            <input type="submit" value="Update" />
            @Html.ActionLink("Back to List", "List", "User")
        </p>
    </fieldset>
}

@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
