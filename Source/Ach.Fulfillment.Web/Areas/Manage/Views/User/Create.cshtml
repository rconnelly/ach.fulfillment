@model Ach.Fulfillment.Web.Areas.Manage.Models.UserModel

@{
    ViewBag.Title = "Create User";
}

@using (Html.BeginForm("Create", "User", new { area = "Manage" }))
{
    <fieldset>
        <legend>Create User</legend>

        @Html.Partial("_userDetails", this.Model)
        
         <div class="btn-toolbar">
            <div class="btn-group">
                <input type="submit" value="Create" class="btn btn-large" />
                @Html.ActionLink("Back to List", "List", "User", new { @class = "btn btn-large" })
             </div>
        </div>
    </fieldset>
}

@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
