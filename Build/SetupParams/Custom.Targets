<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <CloudTargetProfile>Local</CloudTargetProfile>
    </PropertyGroup>
    <ItemGroup>
        <Tokens Include="DatabaseConnection" Condition="'$(DatabaseConnection)'!=''">
            <ReplacementValue>$(DatabaseConnection)</ReplacementValue>
        </Tokens>
        <Tokens Include="CloudTargetProfile">
            <ReplacementValue>$(CloudTargetProfile)</ReplacementValue>
        </Tokens>
    </ItemGroup>
  
</Project>